{
  "Serilog": {
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "TelegramSerilog",
        "Args": {
          "apiKey": "12345:AAAAAAAAAAAA",
          "chatIds": [ "12345", "12346" ],
          "restrictedToMinimumLevel": "Warning",
          "batchTextLength": 1500,
          "batchInterval": 5,
          "maxCapacity": 10,
          "ExcludedByContains": [
            "Failed to determine the https port for redirect"
          ]
        }
      }
    ]
  }
}
